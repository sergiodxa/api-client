{
  "name": "@sergiodxa/api-client",
  "version": "1.0.2",
  "description": "A strongly typed API client using Zod",
  "browser": "./browser/index.js",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tshy",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore --fix --no-error-on-unmatched-pattern ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiodxa/api-client.git"
  },
  "keywords": [],
  "author": "Sergio Xalambr√≠ <hello@sergiodxa.com> (https://sergiodxa.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergiodxa/api-client/issues"
  },
  "homepage": "https://github.com/sergiodxa/api-client#readme",
  "dependencies": {
    "inflected": "^2.1.0"
  },
  "peerDependencies": {
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/inflected": "^1.1.29",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unicorn": "^45.0.0",
    "happy-dom": "^9.10.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "tshy": "^1.8.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1",
    "zod": "^3.22.4"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  }
}
